class 'NobzTactic' (OrkInfantryTactic)

function NobzTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Nobz Tactic")
	
	-- Force melee stance (Commander detaching can cause false stances)
	self.iForceMeleeStance = true
end